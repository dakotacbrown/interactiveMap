QString parser::loadFile(){
    if (!doc->open(QIODevice::ReadOnly | QIODevice::Text)) {
       return NULL;
    }
   
    QByteArray total;
    QByteArray line;
    while (!doc->atEnd()) {
       line = file.read(1024);
       total.append(line);
    }
   
    return QString(total);
}

